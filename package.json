{
  "name": "micronutrient-support-tool",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-prod": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng serve --prod",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "lint": "eslint \"*/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"*/**/*.{ts,tsx}\" --fix",
    "lint:quiet": "eslint \"*/**/*.{ts,tsx}\" --quiet --fix",
    "e2e": "ng e2e",
    "ci:cypress-run": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng run app:cypress-run",
    "cypress:open": "cypress open --browser chrome",
    "cypress:run": "cypress run --browser chrome",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/micronutrient-support-tool/stats.json",
    "prepare": "husky install"
  },
  "private": true,
  "browser": {
    "fs": false
  },
