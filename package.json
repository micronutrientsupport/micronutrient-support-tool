{
  "name": "micronutrient-support-tool",
  "version": "1.6.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-prod": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng serve --configuration production",
    "start-cypress": "node --max-old-space-size=8192 ./node_modules/@angular/cli/bin/ng serve --configuration production,cypress",
    "build": "ng build",
    "build:prod": "ng build --configuration=production",
    "build:staging-branch": "ng build --configuration=production,staging-branch",
    "build:staging-develop": "ng build --configuration=production,staging-develop",
    "build:qa": "ng build --configuration=production,qa",
    "test": "ng test",
    "lint": "eslint \"*/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"*/**/*.{ts,tsx}\" --fix",
    "lint:quiet": "eslint \"*/**/*.{ts,tsx}\" --quiet --fix",
    "e2e": "ng e2e",
    "ci:cypress-run": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng run app:cypress-run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/micronutrient-support-tool/stats.json",
    "prepare": "husky install",
    "start-pwa": "ng build --configuration production && http-server -p 8080 -c-1 dist/micronutrient-support-tool",
    "pwa-gen-assets": "npx pwa-asset-generator src/assets/images/maps-logo-full-white.svg src/assets/icons --background \"#9b51e0\" --index src/index.html --manifest src/manifest.webmanifest --mstile"
  },
  "private": true,
  "browser": {
    "fs": false
  },
  "dependencies": {
